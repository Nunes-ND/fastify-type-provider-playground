{
	"name": "fastify-type-provider-playground",
	"private": "true",
	"version": "1.0.0",
	"description": "A playground for studying Fastify schema providers",
	"engines": {
		"node": ">=22.0.0"
	},
	"scripts": {
		"dev:json-schema-to-ts": "npm run dev -w json-schema-to-ts",
		"dev:zod": "npm run dev -w zod",
		"dev:typebox": "npm run dev -w typebox",
		"build": "tsc --build",
		"test": "npm run test:json-schema-to-ts && npm run test:zod && npm run test:typebox",
		"test:json-schema-to-ts": "SERVER_MODULE='json-schema-to-ts/src/http/server' vitest run",
		"test:watch:json-schema-to-ts": "SERVER_MODULE='json-schema-to-ts/src/http/server' vitest",
		"test:zod": "SERVER_MODULE='zod/src/http/server' vitest run",
		"test:watch:zod": "SERVER_MODULE='zod/src/http/server' vitest",
		"test:typebox": "SERVER_MODULE='typebox/src/http/server' vitest run",
		"test:watch:typebox": "SERVER_MODULE='typebox/src/http/server' vitest"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Nunes-ND/fastify-type-provider-playground.git"
	},
	"keywords": [
		"fastify",
		"typescript",
		"playground"
	],
	"author": "",
	"license": "ISC",
	"type": "commonjs",
	"workspaces": [
		"modules/*"
	],
	"bugs": {
		"url": "https://github.com/Nunes-ND/fastify-type-provider-playground/issues"
	},
	"homepage": "https://github.com/Nunes-ND/fastify-type-provider-playground#readme",
	"devDependencies": {
		"@biomejs/biome": "^2.1.2",
		"@types/node": "^24.1.0",
		"pino-pretty": "^13.0.0",
		"tsx": "^4.20.3",
		"typescript": "^5.8.3",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4"
	}
}
